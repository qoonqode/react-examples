"column":2},"source":"components/main.jsx","original":{"line":8,"column":18}},{"name":"App","generated":{"line":108,"column":15},"source":"components/main.jsx","original":{"line":67,"column":15}}],"sources":{"components/main.jsx":"import React, { Component } from 'react';\nimport { Grid, Input, Header, Button, Segment, Icon } from 'semantic-ui-react';\nimport Octokit from '@octokit/rest';\n\n// CSS\nimport styles from './main.css';\n\nclass App extends Component {\n\n    // Constructor (State!)\n    constructor(props) {\n        super(props);\n        this.state = {\n            repos: [],\n            loading: false,\n            notFound: false\n        }\n    }\n\n    fetchRepository(username) {\n        const octokit = new Octokit();\n        octokit.repos.listForUser({ username, type: 'owner' })\n            .then(({ data }) => {\n                console.log(`Repos:\\n${JSON.stringify(data)}`);\n            })\n            .catch((err) => {\n                console.log(`Unable to fetch repos: ${err}`);\n            });\n    }\n\n    componentDidMount() {\n        // Test!\n        this.fetchRepository('qoonqode');\n    }\n\n    render() {\n        return (\n            <div className={styles.topmargin}>\n                <Grid centered columns={1} divided='vertically'>\n                    <Grid.Row>\n                        <Grid.Column width={8}>\n                            <Grid>\n                                <Header>GitHub Repository Lister!</Header>\n                                <Grid.Row>\n                                    <Grid.Column width={12}>\n                                        <Input placeholder='GitHub Username...' fluid />\n                                    </Grid.Column>\n                                    <Grid.Column width={4}>\n                                        <Button fluid primary>Fetch List!</Button>\n                                    </Grid.Column>\n                                </Grid.Row>\n                            </Grid>\n                        </Grid.Column>\n                    </Grid.Row>\n                    <Grid.Row>\n                        <Grid.Column width={8}>\n\n                        </Grid.Column>\n                    </Grid.Row>\n                </Grid>\n            </div>\n        )\n    }\n\n}\n\nexport default App;"},"lineCount":null}},"error":null,"hash":"22f661561871d69a524ba3cf59af69e9","cacheData":{"env":{}}}